@model Birth



<form action="BirthResult" method="GET">
    <div class="mb-3">
        <label class="form-label">Imię</label>
        <input class="form-control" type="text" name="Name" required placeholder="Wprowadź swoje imię"/>
    </div>
    <div class="mb-3">
        <label class="form-label" >Data urodzenia</label>
        <input class="form-control" type="date" name="DateOfB"  required/>
    </div>
    <div class="mb-3">
        <button  type="submit">Oblicz</button>
    </div>
</form>